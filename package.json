{
  "name": "superpunter_api",
  "version": "1.0.0",
  "description": "Super Punter API built with Node.JS",
  "main": "app.js",
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "scripts": {
    "build": "npx babel -d ./build ./src -s ",
    "migrate": "npx sequelize db:migrate",
    "undo-migrate": "npx sequelize db:migrate:undo:all",
    "heroku-postbuild": "npm run migrate",
    "dev": "nodemon --exec babel-node src/app.js",
    "prestart": "npm run build",
    "start": "node ./build/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tboy44wiz/super-punter.git"
  },
  "keywords": [
    "super",
    "punter",
    "node",
    "api"
  ],
  "author": "Osondu Tochukwu (tMan)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tboy44wiz/super-punter/issues"
  },
  "homepage": "https://github.com/tboy44wiz/super-punter#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.30.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.4",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemailer": "^6.7.5",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "otpauth": "^7.1.2",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.19.0",
    "sequelize-cli": "^6.4.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/node": "^7.17.10",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.17.10",
    "@babel/register": "^7.17.7",
    "nodemon": "^2.0.16"
  }
}
